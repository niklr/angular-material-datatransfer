{
  "name": "angular-material-datatransfer-app",
  "description": "A HTML5 datatransfer UI for handling upload and download of multiple simultaneous files.",
  "version": "4.0.3",
  "keywords": [
    "angular",
    "material-design",
    "uploader",
    "downloader",
    "datatransfer",
    "responsive",
    "web"
  ],
  "author": "https://github.com/niklr",
  "main": "dist/amd-app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niklr/angular-material-datatransfer.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-watch": "ng build --watch",
    "build-prod": "ng build --prod --output-hashing=none",
    "build-lib": "ng build amd-lib && node ./scripts/scss-bundle.build.js",
    "package-es5": "cd dist/amd-app && type runtime-es5.js polyfills-es5.js main-es5.js > angular-material-datatransfer-es5.js",
    "package-es2015": "cd dist/amd-app && type runtime-es2015.js polyfills-es2015.js main-es2015.js > angular-material-datatransfer-es2015.js",
    "append-banner": "node ./scripts/prepend.build.js",
    "bundle": "npm run build-prod amd-app && npm run package-es5 && npm run package-es2015 && npm run append-banner",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "crypto-browserify": "github:niklr/crypto-browserify#7f681b3",
    "file-saver": "github:eligrey/FileSaver.js#5ed507e",
    "hammerjs": "^2.0.8",
    "resumablejs": "github:23/resumable.js#2d5e06b",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.1.0",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^10.1.1",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.0.2",
    "ts-node": "~8.5.4",
    "tsickle": "^0.37.1",
    "tslint": "~5.20.1",
    "typescript": "^3.5.3"
  }
}
